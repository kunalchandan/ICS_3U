import java.util.*;
public class AllArrays1
{
  public static void main(String[]args)
  {
    Scanner input = new Scanner(System.in);
    System.out.println("");
    int n = input.nextInt();
    int[] array = new int[n];
    for(int x = 0; x < n; x++)
    {
      array[x] = 1;
      System.out.println(array[x]);
    }
  }
}
    